# Content Matrix Deployment Configuration
# Copy this to .env and update with your values

# Contract addresses (will be set after deployment)
NEXT_PUBLIC_CONTENT_MATRIX_ADDRESS=
NEXT_PUBLIC_CONTENT_PROTECTION_ADDRESS=

# Network configuration
NEXT_PUBLIC_NETWORK_NAME=sepolia
NEXT_PUBLIC_CHAIN_ID=11155111

# Wallet Connect (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=2ec9743d0d0cd7fb94dee1a7e6d33475

# Sepolia Testnet RPC URL (get from https://infura.io/ or https://alchemy.com/)
SEPOLIA_URL=https://sepolia.infura.io/v3/b18fb7e6ca7045ac83c41157ab93f990

# Private key for deployment (make sure this account has Sepolia ETH)
PRIVATE_KEY=

# Development Settings
NODE_ENV=development
